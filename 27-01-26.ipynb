   "source": [
    "def left_rotate(arr, k):\n",
    "    n = len(arr)\n",
    "    if n == 0:\n",
    "        return arr\n",
    "    k = k % n\n",
    "    return arr[k:] + arr[:k]\n",
    "\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "k = 2\n",
    "\n",
    "print(left_rotate(arr, k))\n"
   ]
